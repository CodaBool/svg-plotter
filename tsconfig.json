        "module": "commonjs",
        "outDir": "dist",
        "noImplicitAny": true,
        "removeComments": true,
        "sourceMap": true,
    },
    "include": [
        "src/**/*",
        "typings/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}